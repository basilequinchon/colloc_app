<div class="row">
  <div class="col-10">
    <% if @user.photo.attached? %>
      <%= cl_image_tag @user.photo.key, alt: "#{@user.first_name}-profile-pic", width: 200, height: 200, crop: :fill %>
    <% else %>
      <%= image_tag "profil.jpg", alt: "default-profile-pic", width: 200, height: 200, crop: :fill %>
    <% end %>
    <h1><%= @user.first_name %> <%= @user.last_name %></h1>
    <p><%= @user.email %></p>
    <p><%= link_to 'Edit profile', edit_user_registration_path %></p>
    <p><%= link_to "Log out", destroy_user_session_path, method: :delete %></p>
    <p><%= link_to 'Home', root_url %></p>
  </div>
</div>
